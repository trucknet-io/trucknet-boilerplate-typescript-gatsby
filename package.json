{
  "name": "trucknet-boilerplate-typescript-react",
  "description": "A starter kit for TypeScript-based Gatsby projects with i18n and Netlify CMS.",
  "version": "0.0.0",
  "private": true,
  "author": "Alexander Gusev <goooseman@me.com>",
  "keywords": [
    "gatsby",
    "trucknet",
    "typescript",
    "boilerplate",
    "react"
  ],
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "dev": "gatsby develop",
    "start": "npm run dev",
    "precommit": "lint-staged && npm run lint",
    "commit": "commit-prompt --no-verify",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.9.0"
  },
  "dependencies": {
    "@material-ui/styles": "^4.1.2",
    "classnames": "^2.2.6",
    "gatsby": "^2.13.2",
    "gatsby-image": "^2.2.4",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-material-ui": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.2",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.2",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-remark-responsive-iframe": "^2.2.1",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "prettier": "^1.18.2",
    "prettier-config-trucknet-io": "^2.0.1",
    "rimraf": "^2.6.3",
    "trucknet-commit": "^1.4.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-config-trucknet-io": "^1.6.0"
  }
}
