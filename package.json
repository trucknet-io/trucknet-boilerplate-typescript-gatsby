{
  "name": "trucknet-boilerplate-typescript-react",
  "description": "A starter kit for TypeScript-based Gatsby projects with i18n and Netlify CMS.",
  "version": "0.0.0",
  "private": true,
  "author": "Alexander Gusev <goooseman@me.com>",
  "keywords": [
    "gatsby",
    "trucknet",
    "typescript",
    "boilerplate",
    "react"
  ],
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "dev": "gatsby develop",
    "start": "npm run dev",
    "precommit": "lint-staged && npm run lint",
    "commit": "commit-prompt --no-verify",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.9.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "babel-plugin-styled-components": "^1.10.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.3.33",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-canonical-urls": "^2.0.12",
    "gatsby-plugin-emotion": "^4.0.6",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.18",
    "polished": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "prettier": "^1.18.0",
    "rimraf": "^2.6.3",
    "trucknet-commit": "^1.4.3",
    "tslint": "^5.17.0",
    "tslint-config-trucknet-io": "^1.5.2"
  }
}
