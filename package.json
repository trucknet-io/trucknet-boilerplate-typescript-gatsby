{
  "name": "trucknet-boilerplate-typescript-react",
  "description": "A starter kit for TypeScript-based Gatsby projects with i18n and Netlify CMS.",
  "version": "0.0.0",
  "private": true,
  "author": "Alex Zinkevych <alexdarkmm@gmail.com>",
  "keywords": [
    "gatsby",
    "trucknet",
    "typescript",
    "boilerplate",
    "react"
  ],
  "scripts": {
    "pretest": "gettext-utils import-strings",
    "prestart": "gettext-utils import-strings",
    "prebuild": "gettext-utils import-strings",
    "prestorybook": "gettext-utils import-strings",
    "precommit": "gettext-utils export-strings --default-locale=en && git add src/i18n/*.po* && lint-staged && npm run lint",
    "prerelease": "gettext-utils validate-strings",
    "commit": "commit-prompt --no-verify",
    "build": "env-cmd -e production gatsby build",
    "build:clean": "npm run clean && npm run build",
    "clean": "rimraf public .cache .awcache node_modules/.cache",
    "dev": "env-cmd -e development gatsby develop",
    "start": "npm run dev",
    "start:clean": "clear && npm run clean && npm start",
    "serve": "gatsby serve",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "jest --detectOpenHandles",
    "test:ci": "npm run test -- --coverage",
    "release": "./config/deployment/release.sh",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.9.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "@sentry/browser": "^5.5.0",
    "@sentry/webpack-plugin": "^1.7.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.27",
    "env-cmd": "^9.0.3",
    "gatsby": "^2.13.13",
    "gatsby-image": "^2.2.4",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-catch-links": "^2.1.0",
    "gatsby-plugin-material-ui": "^2.1.4",
    "gatsby-plugin-netlify": "^2.1.0",
    "gatsby-plugin-netlify-cms": "^4.1.2",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.3",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-remark-responsive-iframe": "^2.2.1",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.3",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.6.2",
    "gatsby-transformer-sharp": "^2.2.1",
    "gettext-utils": "^1.2.1",
    "jss-rtl": "^0.2.3",
    "lioness": "^0.2.13",
    "netlify-cms-app": "^2.9.5",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "rimraf": "^2.6.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.1.9",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-backgrounds": "5.1.9",
    "@storybook/addon-console": "^1.1.1",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/react": "5.1.9",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-loader": "^0.5.5",
    "jest": "^24.8.0",
    "jest-date-mock": "^1.0.7",
    "markdown-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "prettier-config-trucknet-io": "^2.0.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "storybook-addon-i18n": "5.1.9",
    "trucknet-commit": "^1.4.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-trucknet-io": "^1.6.0",
    "webpackbar": "^3.2.0"
  }
}
